							<outline text="{">
								<outline text="method: 'POST',"/>
								<outline text="body: JSON.stringify(data)"/>
								<outline text="}"/>
								</outline>
							<outline text=");"/>
							</outline>
						<outline text="return response.json();"/>
						<outline text="},"/>
						</outline>
